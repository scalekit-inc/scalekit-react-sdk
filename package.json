{
  "name": "@scalekit-sdk/react",
  "version": "0.1.0",
  "description": "React SDK for Scalekit OIDC authentication",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "oidc-client-ts": "^3.4.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.2.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "happy-dom": "^20.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^2.1.0"
  },
  "keywords": [
    "scalekit",
    "oidc",
    "oauth",
    "authentication",
    "react",
    "sso",
    "pkce"
  ],
  "engines": {
    "node": ">=18"
  },
  "author": "Scalekit",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/scalekit/scalekit-react-sdk"
  }
}
