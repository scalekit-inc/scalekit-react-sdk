# Scalekit Configuration
# Copy this file to .env and fill in your Scalekit credentials
# Get your credentials from https://app.scalekit.com

# Your Scalekit environment URL (e.g., https://yourorg-xxxxx.scalekit.cloud)
VITE_SCALEKIT_ENVIRONMENT_URL=https://your-tenant.scalekit.cloud

# Your OAuth Client ID from Scalekit dashboard
VITE_SCALEKIT_CLIENT_ID=your_client_id_here

# Redirect URI after authentication (must match your Scalekit app configuration)
VITE_SCALEKIT_REDIRECT_URI=http://localhost:5173/callback

# Redirect URI after logout (must match your Scalekit app configuration)
VITE_SCALEKIT_POST_LOGOUT_REDIRECT_URI=http://localhost:5173
